a=int(input("Enter the year:"))
b=a-2000
c=b//4
d=b-c
e=c*2
f=d+e
g=f%7
print(g)
if(g==0):
    print("sunday")
if(g==1):
    print("monday")
if(g==2):
    print("tuesday")
if(g==3):
    print("wednesday")
if(g==4):
    print("thursday")
if(g==5):
    print("fridady")
if(g==6):
    print("saturday")
